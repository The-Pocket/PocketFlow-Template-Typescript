{
  "name": "pocketflow-template-typescript",
  "version": "0.1.0",
  "description": "TypeScript template for PocketFlow",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "dev": "nodemon --exec ts-node src/index.ts",
    "start": "ts-node src/index.ts",
    "test": "jest",
    "lint": "eslint src --ext .ts,.js,.jsx,.tsx",
    "format": "prettier --write \"src/**/*.{ts,js,jsx,tsx}\""
  },
  "keywords": [
    "pocketflow",
    "template",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@stylistic/eslint-plugin": "^4.2.0",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.16.0",
    "@types/prompt-sync": "^4.2.3",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "eslint": "^9.23.0",
    "globals": "^16.0.0",
    "jest": "^29.5.0",
    "nodemon": "^3.0.3",
    "prettier": "^2.8.8",
    "prompt-sync": "^4.2.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.28.0"
  },
  "dependencies": {
    "dotenv": "^16.4.7",
    "openai": "^4.90.0",
    "pocketflow": "^1.0.4"
  }
}
